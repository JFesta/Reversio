{
  "Jobs": [
    {
      "Provider": "System.Data.SqlClient",
      "ConnectionString": "",
      "Steps": [
        {
          "StepType": "Load",
          "EntityTypes": [ "Table" ],
          "Schemas": [ "dbo" ],
          "Exclude": {
            "ExcludeExact": [],
            "ExcludeRegex": [ "^(AspNet)", "^(Log)", "^(BKP_)" ]
          }
        },
		{
          "StepType": "Load",
          "EntityTypes": [ "View" ],
          "Schemas": [ "dbo" ],
          "Exclude": {
            "ExcludeExact": [ "dbo.ViewToExclude" ],
            "ExcludeRegex": []
          }
        },
        {
          "StepType": "PocoGenerate",
          "Namespace": "MyProject.Model",
          "ClassAccessModifier": "public",
          "ClassPartial": true,
          "VirtualNavigationProperties": false,
          "Usings": [],
          "Extends": [],
          "ClassNameReplace": [
            {
              "Regex": "^(vw_)",
              "ReplaceWith": "Vw"
            },
            {
              "Regex": "^(sp_)",
              "ReplaceWith": ""
            },
            {
              "Regex": "^(usp_)",
              "ReplaceWith": ""
            },
            {
              "Regex": "^(fn_)",
              "ReplaceWith": ""
            },
            {
              "Regex": "_",
              "ReplaceWith": ""
            }
          ],
          "ClassNameForcePascalCase": true,
          "PropertyNameReplace": [
            {
              "Regex": "_",
              "ReplaceWith": ""
            }
          ],
          "PropertyNameForcePascalCase": true,
          "PropertyNullableIfDefaultAndNotPk": false
        },
        {
          "StepType": "PocoWrite",
          "OutputPath": "./",
          "CleanFolder": true,
          "Exclude": {
            "ExcludeExact": [],
            "ExcludeRegex": []
          }
        },
        {
          "StepType": "DbContext",
          "OutputPath": "./Model/MyDbContext.cs",
          "Namespace": "MyProject.Model",
          "ClassName": "MyDbContext",
          "Extends": [ "DbContext" ],
          "IncludeIndices": true
        }
      ]
    }
  ]
}
